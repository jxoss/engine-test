{
    "flow": {
        "ready": {
            "d": [
                [":render", {"tmpl": "layout"}],
                ">>showFlowDetail"
            ]
        },
        "showFlowDetail": {
            "d": [
                [":fd_layout/render", {"tmpl": "layout"}],
                [":state", {"name": "content", "tmpl": "layout"}]
            ]
        },
        "showTests": {
            "d": [
                [":layout_test/render", {"tmpl": "layout"}],
                [":state", {"name": "content", "tmpl": "layout"}]
            ]
        }
    },
    "config": {
        "title": "Engine Test",
        "templates": {
            "layout": {
                "to": "body",
                "css": [
                    "https://fonts.googleapis.com/icon?family=Material+Icons",
                    "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css"
                ],
                "html": "/layout.html",
                "events": {
                    "showFlowDetail": [
                        {
                            "on": "click",
                            "sel": "#showFlowDetail"
                        }
                    ],
                    "showTests": [
                        {
                            "on": "click",
                            "sel": "#showTests"
                        }
                    ]
                }
            }
        },
        "states": {
            "content": [
                {
                    "sel": ".loader",
                    "template": "layout",
                    "add": ["hide"]
                },
                {
                    "sel": ".section",
                    "template": "layout",
                    "rm": ["hide"]
                }
            ],
            "loader": [
                {
                    "sel": ".section",
                    "template": "layout",
                    "add": ["hide"]
                },
                {
                    "sel": ".loader",
                    "template": "layout",
                    "rm": ["hide"]
                }
            ]
        }
    },
    "roles": {
        "*": true
    },
    "name": "layout",
    "module": "view"
}
