{
    "flow": {
        "ready": [
            [
                [":render", {"tmpl": "layout"}],
                [":tests/render", {"tmpl": "layout"}],
                [">>getTests", {"to": "api", "net": "ws"}],
                [":tests/render", {"tmpl": "list"}]
            ]
        ]
    },
    "config": {
        "title": "Engine Test",
        "templates": {
            "layout": {
                "to": "body",
                "html": "/layout.html"
            }
        },
        "domEvents": [
            {
                "on": "click",
                "selector": "#my_clickable_1",
                "flow": "getTests"
            }
        ],
        "states": {
            "content": [
                {
                    "sel": ".loader",
                    "template": "layout",
                    "add": ["hide"]
                },
                {
                    "sel": ".section",
                    "template": "layout",
                    "rm": ["hide"]
                }
            ],
            "loader": [
                {
                    "sel": ".section",
                    "template": "layout",
                    "add": ["hide"]
                },
                {
                    "sel": ".loader",
                    "template": "layout",
                    "rm": ["hide"]
                }
            ]
        }
    },
    "markup": [
      "/layout.html"
    ],
    "styles": [
      "/bootstrap/css/bootstrap.min.css",
      "/bootstrap/css/bootstrap-theme.min.css",
      "/style.css"
    ],
    "roles": {
        "*": true
    },
    "name": "layout",
    "module": "view"
}
