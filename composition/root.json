{
  "client": {
    "config": {
        "title": "Starter Project",
        "templates": {
            "layout": {
                "to": "body",
                "html": "/root.html",
                "render": true
            }
        },
        "defaultTemplate": "layout",
        "states": {
            "test": [
                {
                    "sel": ".container",
                    "template": "layout",
                    "toggle": ["well"]
                }
            ]
        },
        "flow": [
            {
                "on": "click",
                "selector": "#test-flow",
                "event": "test-flow-ev"
            },
            {
                "on": "click",
                "selector": "#test-reload",
                "event": "test-reload"
            }
        ]
    },
    "flow": [
        [
            "test-flow-ev",
            ["flow-tester/test"]
        ],
        [
            "test-reload",
            [":reload"]
        ]
    ],
    "markup": [
      "/root.html"
    ],
    "styles": [
      "/bootstrap/css/bootstrap.min.css",
      "/bootstrap/css/bootstrap-theme.min.css",
      "/style.css"
    ]
  },
  "roles": {
    "*": true
  },
  "name": "root",
  "module": "view"
}
