{
  "client": {
    "config": {
        "title": "Starter Project",
        "templates": {
            "layout": {
                "to": "body",
                "html": "/root.html",
                "render": true
            }
        },
        "defaultTemplate": "layout",
        "states": {
            "test": [
                {
                    "sel": ".container",
                    "template": "layout",
                    "toggle": ["well"]
                }
            ]
        },
        "flow": [
            {
                "on": "click",
                "selector": "#test-flow",
                "flow": [
                    ["custom/test.flow"]
                ]
            },
            {
                "on": "click",
                "selector": "#test-instance",
                "flow": [
                    [":reload"]
                ]
            }
        ]
    },
    "markup": [
      "/root.html"
    ],
    "styles": [
      "/bootstrap/css/bootstrap.min.css",
      "/bootstrap/css/bootstrap-theme.min.css",
      "/style.css"
    ]
  },
  "roles": {
    "*": true
  },
  "name": "root",
  "module": "view"
}
