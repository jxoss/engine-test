{
    "name": "fd_ds_list",
    "module": "view",
    "roles": {
        "*": true
    },
    "config": {
        "templates": {
            "layout": {
                "to": ".flow-ds-list",
                "html": "/fd_ds_item.html",
                "events": {
                    "showDetail": [
                        {
                            "on": "click",
                            "sel": ".flow-dh"
                        }
                    ]
                }
            }
        }
    },
    "flow": {
        "showDetail": {
            "d": [
                [">>showDetail", {"to": "fd_ds_layout"}]
            ]
        }
    }
}
